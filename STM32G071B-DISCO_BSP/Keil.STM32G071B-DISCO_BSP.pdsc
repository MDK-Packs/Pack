    <!-- VIO component for STM32G071B-DISCO -->
    <component Cclass="CMSIS Driver" Cgroup="VIO" Csub="Board" Cvariant="STM32G071B-DISCO" Cversion="1.2.0" Capiversion="0.1.0" condition="STM32G071B-DISCO VIO">
