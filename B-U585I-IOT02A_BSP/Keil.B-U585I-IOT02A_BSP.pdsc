      <require Cclass="Board Support" Cgroup="Drivers"       Csub="BUS"/>
      <require Cclass="Board Support" Cgroup="Components"    Csub="OV5640"/>
    </condition>
    <condition id="B-U585I-IOT02A BSP EEPROM">
