      <require Cclass="Board Support" Cgroup="Drivers"       Csub="BUS"/>
      <require Cclass="Board Support" Cgroup="Components"    Csub="VEML6030"/>
    </condition>
    <condition id="B-U585I-IOT02A BSP MOTION SENSORS">
